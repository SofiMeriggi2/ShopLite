{
  "name": "@shoplite/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset -f",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate",
    "start": "nest start",
    "migrate": "prisma migrate dev --schema apps/backend/prisma/schema.prisma",
    "generate": "prisma generate --schema apps/backend/prisma/schema.prisma",
    "studio": "prisma studio --schema apps/backend/prisma/schema.prisma",
    "seed": "ts-node apps/backend/prisma/seed.ts"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^2.0.0",
    "@anatine/zod-openapi": "^2.2.8",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^6.0.0",
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.0.0",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "openapi3-ts": "^4.5.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.12",
    "jest": "^29.7.0",
    "prisma": "^6.16.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "schema": "apps/backend/prisma/schema.prisma"
  }
}
